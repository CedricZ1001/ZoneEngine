<Window x:Class="ZoneEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZoneEditor"
        xmlns:editors="clr-namespace:ZoneEditor.Editors"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="ZoneEngine" Height="1080" Width="1920"
        Icon="pack://application:,,,/ZoneEditor;component/Resources/ZoneIcon.ico"
        ResizeMode="CanResizeWithGrip"
        Style="{StaticResource ZoneWindowStyle}"
